{
  "version": 2,
  "builds": [
    {
      "src": "vercel.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["dist/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "vercel.js"
    }
  ],
  "functions": {
    "vercel.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}