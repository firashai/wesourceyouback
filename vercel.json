{
  "version": 2,
  "builds": [
    {
      "src": "vercel.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "dist/**",
          "node_modules/**",
          ".env"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "vercel.js"
    }
  ],
  "functions": {
    "api/*.js": {
      "runtime": "@vercel/node@1"
    }
  }
}